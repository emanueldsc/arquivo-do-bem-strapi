{
  "kind": "collectionType",
  "collectionName": "docs",
  "info": {
    "singularName": "doc",
    "pluralName": "docs",
    "displayName": "doc"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "file": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "is_public": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "semester": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::semester.semester",
      "inversedBy": "docs"
    }
  }
}
